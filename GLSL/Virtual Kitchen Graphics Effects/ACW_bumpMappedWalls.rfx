    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec3 Pos;
varying vec3 Normal;
varying vec2 texCoord;
uniform vec3 position;
uniform vec3 size;
uniform float angle;

varying vec3 vNormal;
varying vec2 vTex;
varying vec3 vTangent;
varying vec3 vBinormal;

attribute vec3 rm_Tangent;
attribute vec3 rm_Binormal;

void main( void )
{

   texCoord = gl_MultiTexCoord0.xy;

   vec4 inPos=gl_Vertex;
   inPos.xyz *= size.xyz;
   inPos.xyz += position.xyz;
   
   mat3 rotation = mat3(1.0, 0.0,0.0,

                    0.0, cos (angle) ,-sin(angle) ,

                    0.0, sin(angle) , cos(angle) );
                   
   inPos.xyz = rotation*inPos.xyz;
   
   gl_Position = gl_ModelViewProjectionMatrix * inPos;
   
   
   vTex = vec2(gl_MultiTexCoord0);

   vec3 n = gl_Vertex.xyz;
   
   //n.z *=-1.0;
   //n.z *=-1.0;
   n.z *=-1.0;
   
   // Rotate tangent basis from object/world space to view
   // space (in RenderMonkey lights are defined in view space)
   vNormal    = vec3(gl_NormalMatrix * gl_Normal);
   vTangent   = vec3(gl_NormalMatrix * rm_Tangent);
   vBinormal  = vec3(gl_NormalMatrix * rm_Binormal);
}]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
//Eye position
uniform vec4 eyePos;
uniform vec4 wallColor;
varying vec4 ColorAtVertex;
varying vec3 Pos;
varying vec3 Normal;
varying vec2 texCoord;

uniform sampler2D bump_map;
uniform sampler2D rgb_map;

uniform mat4 r_sh_matrix;
uniform mat4 b_sh_matrix;
uniform mat4 g_sh_matrix;

varying vec3 vNormal;
varying vec2 vTex;
varying vec3 vTangent;
varying vec3 vBinormal;

uniform float A;

// Texture
uniform sampler2D MyTextureObj;

void main(void)
{

vec4 texColor = texture2D(MyTextureObj, texCoord);
gl_FragColor = texColor * wallColor;

   vec4 c;
   mat3 rotation;
   vec3 normal3;
   vec4 normal4;

   // Matrix to transform from tangent space into light space)
   rotation = mat3(normalize(vTangent) , normalize(vBinormal), normalize(vNormal));

   // Get normal
   normal3 = texture2D(bump_map, vTex).xyz * 2.0 - 1.0;
   // the *_sh_matrix'es below are the wrong handedness for opengl, so we need to flip z
   normal3.z *= -1.0;

   // Transform normal into light space
   normal4 = vec4((normal3 * rotation), 1.0);
   
   //Evaluate spherical harmonic
   c.r = dot((r_sh_matrix * normal4), normal4);
   c.g = dot((g_sh_matrix * normal4), normal4);
   c.b = dot((b_sh_matrix * normal4), normal4);
   c.a = 1.0;

   // Multiply by rgb factor (and scale by two)
   gl_FragColor = c *  texture2D(rgb_map, vTex) * 2.0;

}]]></RmShaderCode></RmGLShader>
